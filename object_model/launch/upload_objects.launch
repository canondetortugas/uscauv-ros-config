<launch>
  <group ns="objects" >

    <!-- Settings applied across all objects -->
    <rosparam command="load" param="global" file="$(find object_model)/unimodal/global.yaml" />
    
    <!-- <rosparam command="load" param="pipe" file="$(find object_model)/unimodal/pipe.yaml" /> -->
    <rosparam command="load" param="notecard" file="$(find object_model)/unimodal/notecard.yaml" />
    
  </group>
</launch>