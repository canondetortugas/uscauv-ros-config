<launch>
  <arg name="namespace" default="model" />  

  <!-- List of all of the thrusters we will be simulating -->
  <rosparam command="load" file="$(find auv_model)/dummy_robot/thrusters.yaml"
	    param="names" ns="$(arg namespace)/thrusters" />
  
  <!-- Upload the thruster model for every thruster in the names param -->
  <rosparam command="load" file="$(find auv_model)/dummy_robot/dummy_thruster.yaml"
	    param="thruster1" ns="$(arg namespace)/thrusters" />

  <rosparam command="load" file="$(find auv_model)/dummy_robot/dummy_thruster.yaml"
	    param="thruster2" ns="$(arg namespace)/thrusters" />

  <rosparam command="load" file="$(find auv_model)/dummy_robot/dummy_thruster.yaml"
	    param="thruster3" ns="$(arg namespace)/thrusters" />

  <rosparam command="load" file="$(find auv_model)/dummy_robot/dummy_thruster.yaml"
	    param="thruster4" ns="$(arg namespace)/thrusters" />

  <rosparam command="load" file="$(find auv_model)/dummy_robot/dummy_thruster.yaml"
	    param="thruster5" ns="$(arg namespace)/thrusters" />

  <rosparam command="load" file="$(find auv_model)/dummy_robot/dummy_thruster.yaml"
	    param="thruster6" ns="$(arg namespace)/thrusters" />




  
</launch>